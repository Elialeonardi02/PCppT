T Фигура
   F.virtual.abstract площадь() -> Float
   fn.virtual.abstract площадь2() -> Float

loop 1..Int(input())
   var s = input()
   var m = Int(input())
   var b = input().split(‘ ’).map(Int)
   var array = sorted(Array(Set(Array(s))))
   var answer = [Char("\0")] * m
   L sum(b) != -m
      Set[Int] inds
      loop(i) 0 .< m
         if b[i] == 0
            inds.add(i)
            b[i] = -1
      loop
         var q = array.pop()
         if inds.len <= s.count(q)
            loop(i) inds
               answer[i] = q
            loop.break
      loop(i) inds
         loop(j) 0 .< m
            if b[j] != -1
               b[j] -= abs(i - j)
   print(answer.join(‘’))